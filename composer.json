{
  "name": "crcms/microservice-bridging",
  "description": "Bridge package common to microservice clients and servers.",
  "keywords": [
    "microservice",
    "bridge",
    "laravel",
    "php"
  ],
  "homepage": "http://blog.crcms.cn",
  "support": {
    "issues": "https://github.com/crcms/microservice-bridging/issues",
    "source": "https://github.com/crcms/microservice-bridging"
  },
  "authors": [
    {
      "name": "simon",
      "email": "simon@crcms.cn"
    }
  ],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^7.2.4",
    "ext-json": "*",
    "illuminate/support": "^5.7",
    "illuminate/encryption": "^5.7",
    "vlucas/phpdotenv": "^2.2 | ^3.3"
  },
  "require-dev": {
    "illuminate/container": "^5.7",
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^7.4",
    "symfony/var-dumper": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "CrCms\\Microservice\\Bridging\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CrCms\\Microservice\\Bridging\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
